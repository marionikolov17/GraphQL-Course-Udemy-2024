{
  "overwrite": true,
  "schema": "./schema.graphql",
  "generates": {
    "src/generated/schema.ts": {
      "plugins": ["typescript", "typescript-resolvers"],
      "config": {
        "skipTypename": true,
        "mappers": {
          "Company": "./src/db/types.ts#CompanyEntity",
          "Job": "./src/db/types.ts#JobEntity"
        }
      }
    }
  }
}
